{
    "name": "appkonik/products",
    "description": "Ecommerce Products Package",
    "keywords": [
        "appkonik",
        "products"
    ],
    "homepage": "https://github.com/appkonik/products",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Appkonik LLC",
            "email": "info@appkonik.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "illuminate/support": "^9.48"
    },
    "autoload": {
        "psr-4": {
            "Appkonik\\Products\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Appkonik\\Products\\ProductsServiceProvider"
            ],
            "aliases": {
                "Products": "Appkonik\\Products\\ProductsFacade"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
